{
  "expo": {
    "name": "Vasco",
    "slug": "dailybook",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vascoandco.vasco",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "L'application a besoin d'accéder à votre galerie de photos",
        "NSCameraUsageDescription": "L'application a besoin d'accéder à votre appareil photo",
        "NSUserTrackingUsageDescription": "L'application a besoin d'une permission pour vous envoyez des notifications",
        "UIBackgroundModes": [
          "fetch", "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vascoandco.vasco",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "com.google.android.c2dm.permission.RECEIVE"
      ],
      "kotlinVersion": "1.5.31"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "9733b21a-2b6a-413e-98ff-c4fdc8c1765d"
      }
    },
    "owner": "riujin",
    "plugins": [
      "expo-font",
      "expo-notifications"
    ]
  }
}
